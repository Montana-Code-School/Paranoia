            <li role="separator" class="divider"></li>
            <li><a href="/logout">Logout</a></li>
          </ul>
        </li>
      </ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>
	<div class="container box">
		<div class="row"> 
			<div>
				<h1><%= user.local.handle %>'s Dossier</h1>
				<h2><strong>email</strong>: <%= user.local.email %><br></h2>	
				<h2><strong>Tag:</strong> <%= user.local.tag %> <br></h2>
			</div>
		</div>
	</div>

	<div class="container box-two">
		

		<div class="row">
			<div class="col-xs-12 col-xs-offset-4">
				<h1><span class="glyphicon glyphicon-chevron-right"></span> Game Console</h1>
			</div>
			<div class="col-xs-4" id="imageContainer">
				<!-- <img src="<%=user.local.pic%>" class="agentPicture"> -->
				<img src="http://outlar.com/old_site/images/artists/GuyGerber1.jpg" class="agentPicture">
			</div>

			<div class="col-xs-4">
				<ul>
					<div class="container">
					  <h2>Your target: Cobra </h2>
					  <h3 class="confirmTagText">Enter tag number to confirm elimination:</h3>                                                                               
					</div>
					<div class="col-xs-8">
						<input id="tagGetter" type="text" class="form-control" name="tag" placeholder="Agent Tag #">
					</div>
					<div class="col-xs-3">
						<a class="btn btn-success" href="./profile2.html" role="button">Submit Tag #</a>
						<!-- <button href="../public/profile2.html" class="btn btn-success">Submit Tag #</button> -->
					</div>
					<div class="col-xs-12">
						<br>
						<br>
						<br>
						<a href="/createGame" class="btn btn-default"><span class="fa fa-user"></span>Create Game</a>
					</div>
				</ul>	
			</div>

		</div>
	</div>

	</div>
	<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.0.0-alpha1/jquery.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" integrity="sha512-K1qjQ+NcF2TYO/eI3M6v8EiNYZfA95pQumfvcVrTHtwQVDG+aHRqLi/ETn2uB+1JqwYqVG3LIvdm9lj6imS/pQ==" crossorigin="anonymous"></script>

	<script type="text/javascript" src="/js/pickplayers.js"></script>

