<html>
<head>
	<title></title>
	<!-- Latest compiled and minified CSS -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" integrity="sha512-dTfge/zgoMYpP7QbHy4gWMEGsbsdZeCXz7irItjcC3sPUFtf0kuFbDz/ixG7ArTxmDjLXDmezHubeNikyKGVyQ==" crossorigin="anonymous">
	<link rel="stylesheet" type="text/css" href="/css/main.css">
</head>
<body>
	<div class="container box">
		<div class="row"> 
			<div>
				<h1><%= user.local.handle %>'s Dossier</h1>
					
				<strong>email</strong>: <%= user.local.email %><br>
					
				strong>Tag:   </strong> <br>
			</div>
		</div>
	</div>

	<div class="container box-two">
		<!-- <div class="row" id="AgentPicture">
			<div class="col-xs-offset-1 col-xs-4">
				<img src="<%=player.pic%>">
			</div>
		</div> -->

		<div class="row">
			<div class="col-xs-4" id="imageContainer">
				<img src="/images/skull.png" class="fakePhotoForSizing">
			</div>

			<div class="col-xs-4">
				<ul>
					<h1><span class="glyphicon glyphicon-chevron-right"></span>Game Console</h1>
					<div id="myGame">
					</div>

					<div class="container">
					  <h2>Agents in the Field</h2>
					  <p>agents fighting</p>                                                                                  
					</div>
						<a href="/createGame" class="btn btn-default"><span class="fa fa-user"></span>Create Game</a>
						<a href="/logout" class="btn btn-default"><span class="fa fa-user"></span>Log Out</a>

				</ul>	
			</div>
		</div>
	</div>

		<div>
			<span class="input-group-addon" id="basic-addon1">Confirm Target Elimination</span>
			<input id="tagGetter" type="text" class="form-control" name="tag" placeholder="Agent Tag">

			<button type="submit" onClick="grabTag('<%= player.tag %>')" class="btn btn-warning btn-lg">Enter Tag</button>
		</div>

	</div>
	<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.0.0-alpha1/jquery.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" integrity="sha512-K1qjQ+NcF2TYO/eI3M6v8EiNYZfA95pQumfvcVrTHtwQVDG+aHRqLi/ETn2uB+1JqwYqVG3LIvdm9lj6imS/pQ==" crossorigin="anonymous"></script>

	<script type="text/javascript" src="/js/pickplayers.js"></script>

</body>
</html>